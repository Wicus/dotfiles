{
  "$schema": "https://raw.githubusercontent.com/LGUG2Z/komorebi/v0.1.20/schema.json",
  "app_specific_configuration_path": "$Env:USERPROFILE/.config/komorebi/applications.yaml",
  "window_hiding_behaviour": "Cloak",
  "cross_monitor_move_behaviour": "Insert",
  "default_workspace_padding": 0,
  "default_container_padding": -8,
  "active_window_border": false,
  "active_window_border_colours": {
    "single": { "r": 66, "g": 165, "b": 245 },
    "stack": { "r": 256, "g": 165, "b": 66 },
    "monocle": { "r": 255, "g": 51, "b": 153 }
  },
  "float_rules": [
    {
      "kind": "Exe",
      "id": "FortiClient.exe"
    },
    {
      "kind": "Title",
      "id": "Control Panel"
    },
    {
      "kind": "Title",
      "id": "Task Manager"
    },
    {
      "kind": "Title",
      "id": "Command Prompt"
    },
    {
      "kind": "Exe",
      "id": "cmd.exe"
    },
    {
      "kind": "Exe",
      "id": "SafeInCloud.exe"
    }
  ],
  "manage_rules": [],
  "monitors": [
    {
      "workspaces": [
        { "name": "I", "layout": "BSP" },
        { "name": "II", "layout": "BSP" },
        {
          "name": "III",
          "custom_layout": "C:/Users/Wicus Pretorius/.config/komorebi/layouts/debugger.json"
        },
        { "name": "IV", "layout": "BSP" },
        { "name": "V", "layout": "BSP" },
        { "name": "VI", "layout": "BSP" },
        { "name": "VII", "layout": "BSP" },
        { "name": "VIII", "layout": "BSP" },
        { "name": "IX", "layout": "BSP" },
        { "name": "X", "layout": "BSP" },
        { "name": "XI", "layout": "BSP" },
        { "name": "III", "layout": "BSP" },
        { "name": "IV", "layout": "BSP" },
        { "name": "V", "layout": "BSP" },
        { "name": "VI", "layout": "BSP" },
        { "name": "VII", "layout": "BSP" },
        { "name": "VIII", "layout": "BSP" },
        { "name": "IX", "layout": "BSP" },
        { "name": "X", "layout": "BSP" },
        { "name": "XI", "layout": "BSP" }
      ]
    }
  ]
}
